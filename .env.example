# Alfred AI Configuration
# Copy this file to .env and update values for your environment

# Ollama Server
# If running Ollama locally on your host machine:
OLLAMA_HOST=http://host.docker.internal:11434
# If running Ollama on another machine:
# OLLAMA_HOST=http://192.168.1.100:11434

# Embedding Model (must be pulled in Ollama first)
EMBEDDING_MODEL=all-minilm

# Qdrant Vector Database
QDRANT_HOST=qdrant
QDRANT_PORT=6333

# Redis Cache
REDIS_HOST=redis
REDIS_PORT=6379

# Alfred AI URL (for external integrations)
ALFRED_AI_URL=http://localhost:8080

# Logging
LOG_LEVEL=INFO
